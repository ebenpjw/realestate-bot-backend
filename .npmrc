# NPM Configuration for Railway Deployment
# Helps prevent cache-related build issues

# Use faster registry
registry=https://registry.npmjs.org/

# Disable audit during install to speed up builds
audit=false

# Disable funding messages
fund=false

# Disable progress bar in CI
progress=false

# Set timeout for network requests
fetch-timeout=60000
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000
