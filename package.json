{
  "name": "realestate-bot-backend",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js | pino-pretty",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "validate": "npm run lint",
    "build": "echo 'Build complete - Ready for deployment'",
    "railway:build": "npm run build",
    "railway:start": "NODE_ENV=production npm start",
    "health-check": "curl -f http://localhost:8080/health || exit 1",
    "db:migrate": "echo 'Run Supabase migrations manually'",
    "postinstall": "echo 'Installation complete - Ready for Railway deployment'"
  },
  "keywords": ["whatsapp", "bot", "real-estate", "ai", "webhook"],
  "author": "Your Name",
  "license": "ISC",
  "description": "AI-powered WhatsApp bot for real estate lead generation and qualification",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "googleapis": "^150.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "node-cache": "^5.1.2",
    "openai": "^5.5.1",
    "pino": "^9.2.0",
    "pino-http": "^8.6.1",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  }
}