{
  "name": "realestate-bot-backend",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "cd frontend && npm install && npm run build",
    "start": "npm run build && node server.js",
    "railway:unified:start": "node server.js",
    "dev": "nodemon index.js | pino-pretty",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "validate": "npm run lint",
    "health-check": "curl -f http://localhost:8080/health || exit 1",
    "db:migrate": "echo 'Run Supabase migrations manually'",
    "dev:unified": "concurrently \"npm run dev\" \"cd frontend && npm run dev\"",
    "test:messages": "node tests/run-messages-tests.js",
    "test:messages:headed": "HEADLESS=false node tests/run-messages-tests.js",
    "test:messages:debug": "HEADLESS=false PWDEBUG=1 npx playwright test tests/messages.spec.js --headed --debug",
    "postinstall": "npm rebuild sharp --platform=linux --arch=x64 || echo 'Sharp rebuild failed, continuing...'",
    "audit": "npm audit --production",
    "test": "node tests/test_master_runner.js",
    "test:quick": "node tests/test_master_runner.js --quick",
    "test:full": "node tests/test_master_runner.js --full --report"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "real-estate",
    "ai",
    "webhook"
  ],
  "author": "Your Name",
  "license": "ISC",
  "description": "AI-powered WhatsApp bot for real estate lead generation and qualification",
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/storage-js": "^2.10.0",
    "@supabase/supabase-js": "^2.50.3",
    "@tailwindcss/forms": "^0.5.10",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.1",
    "express-validator": "^7.2.1",
    "googleapis": "^150.0.1",
    "helmet": "^8.0.0",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.1",
    "node-cron": "^4.2.0",
    "openai": "^5.8.2",
    "path-to-regexp": "^6.2.1",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.3.0",
    "playwright": "^1.54.1",
    "puppeteer": "^24.12.0",
    "react-resizable-panels": "^3.0.3",
    "router": "^1.3.8",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2"
  }
}
