# Production Environment Configuration
# PropertyHub Command Frontend

# Application
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# API Configuration - Production Backend
NEXT_PUBLIC_API_URL=https://realestate-bot-backend-production.up.railway.app/api
NEXT_PUBLIC_WS_URL=wss://realestate-bot-backend-production.up.railway.app

# Supabase Configuration (Production)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Authentication
NEXT_PUBLIC_AUTH_REDIRECT_URL=https://propertyhub-command.netlify.app

# External Services
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
NEXT_PUBLIC_ZOOM_API_KEY=your-zoom-api-key

# PWA Configuration
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your-vapid-public-key

# Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
NEXT_PUBLIC_HOTJAR_ID=your-hotjar-id

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_REAL_TIME=true
NEXT_PUBLIC_ENABLE_TESTING_MODE=false

# Performance
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_BUNDLE_ANALYZER=false

# Security
NEXT_PUBLIC_CSP_NONCE=auto-generated

# Deployment
NETLIFY_SITE_ID=your-netlify-site-id
NETLIFY_AUTH_TOKEN=your-netlify-auth-token
