{
  "name": "propertyhub-command",
  "version": "1.0.0",
  "description": "Apple-inspired frontend for multi-tenant real estate bot system",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 --turbo",
    "build": "node scripts/build-for-railway.js",
    "build:standard": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next dist coverage",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:preview": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh preview",
    "deploy:production": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh production",
    "deploy:build-only": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh build-only",
    "lighthouse": "lighthouse-ci autorun",
    "version": "echo $npm_package_version",
    "verify-setup": "node scripts/verify-setup.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.0",
    "@heroicons/react": "^2.1.0",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.56.0",
    "@tanstack/react-query-devtools": "^5.56.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.5.0",
    "lucide-react": "^0.445.0",
    "next": "^15.0.3",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.47",
    "qrcode.react": "^4.0.1",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.13.0",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "recharts": "^2.12.0",
    "socket.io-client": "^4.8.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2",
    "vaul": "^1.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.11.5",
    "@next/bundle-analyzer": "^15.0.0",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-c8b3f72",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "playwright": "^1.48.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "rimraf": "^6.0.1",
    "vitest": "^2.1.0",
    "workbox-webpack-plugin": "^7.1.0",
    "workbox-window": "^7.1.0"
  }
}
